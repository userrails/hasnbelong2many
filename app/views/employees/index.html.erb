<%= link_to "New employee", new_employee_path %>

<br/>
<% if @employees.present? %>
<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Address</th>
  </tr>
  <% @employees.each_with_index do |e| %>
  <tr>
    <td>
      <%= e.name %>
    </td>
    <td>
      <%= e.age %>
    </td>
    <td>
        <%= e.address.html_safe %>
    </td>
  </tr>
  <% end %>
</table>
<% end %>